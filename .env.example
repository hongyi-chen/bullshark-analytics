# Strava
STRAVA_CLIENT_ID=
STRAVA_CLIENT_SECRET=

# Club-feed MVP (only one person authenticates; server polls the club feed)
STRAVA_CLUB_ID=1318168
# The Strava athlete id of the account you will use as the service account.
# After you OAuth once, set this to the athleteId returned by /api/auth/strava/callback.
STRAVA_SERVICE_ATHLETE_ID=

# App
# e.g. http://localhost:3000 (dev) or https://your-app.vercel.app (prod)
APP_BASE_URL=

# 32-byte key, base64 encoded. Generate with:
#   node -e "console.log(require('crypto').randomBytes(32).toString('base64'))"
APP_ENCRYPTION_KEY=

# Admin/cron endpoint auth
# Optional for local dev (if APP_BASE_URL is localhost).
# Recommended for production:
# - POST /api/cron/club-poll (Authorization: Bearer ...)
# - POST /api/cron/club-poll?secret=... (if your cron cannot set headers)
JOBS_RUNNER_SECRET=

# Database
# Postgres connection string (Neon/Vercel Postgres).
# Example:
# DATABASE_URL=postgresql://USER:PASSWORD@HOST:5432/DB?sslmode=require
DATABASE_URL=
